{
  "expo": {
    "name": "Stelody",
    "slug": "Stelody",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "",
    "scheme": "restate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "INTERNET",
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Stelody"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Rubik-Bold.ttf",
            "./assets/fonts/Rubik-ExtraBold.ttf",
            "./assets/fonts/Rubik-Medium.ttf",
            "./assets/fonts/Rubik-Light.ttf",
            "./assets/fonts/Rubik-Regular.ttf",
            "./assets/fonts/Rubik-SemiBold.ttf",
            "./assets/fonts/BebasNeue-Regular.ttf",
            "./assets/fonts/Oswald-VariableFont_wght.ttf",
            "./assets/fonts/helvetica-light-587ebe5a59211.ttf",
            "./assets/fonts/helvetica-compressed-5871d14b6903a.ttf",
            "./assets/fonts/Helvetica-BoldOblique.ttf",
            "./assets/fonts/Helvetica-Bold.ttf",
            "./assets/fonts/Helvetica.ttf"
          ]
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
